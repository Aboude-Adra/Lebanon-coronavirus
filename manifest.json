{
   "name": "Corona-Lebanon",
   "manifest_version": 2,
   "description": "Displays current Corona stats in Lebanon",
   "version": "1.0",

   "browser_action": {
      "default_icon": "icons/corona_32.png",
      "default_popup": "popup/index.html",
      "default_title": "Corona Lebanon"
   },
   
   "icons": {
      "128": "icons/corona_128.png",
      "48": "icons/corona_48.png",
      "96": "icons/corona_96.png"
   }
}
